<%= form_for(@restaurant, :url => {:action => "create_restaurant"}) do |res| %>
name : <%= res.text_field :resname %>
phone : <%= res.text_field :phone %>
information : <%= res.text_field :resinfo %>

<select name="restaurant[restype]">
    <option value="1">중식</option>
    <option value="2">도시락</option>
    <option value="3">치킨</option>
    <option value="4">피자</option>
    <option value="5">기타</option>

</select>

<%= submit_tag("submit") %>
<% end %>
